; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Overlay"
run/main_scene="res://Overlay/Main.tscn"
run/disable_stdout=true
config/features=PackedStringArray("4.1", "C#", "Forward Plus")
boot_splash/show_image=false

[audio]

buses/default_bus_layout="res://Overlay/Audios/BusLayout.tres"
general/text_to_speech=true

[debug]

gdscript/warnings/unused_parameter=0

[display]

window/size/viewport_width=2560
window/size/viewport_height=1440
window/size/resizable=false
window/size/borderless=true
window/size/transparent=true
window/subwindows/embed_subwindows=false
window/per_pixel_transparency/allowed=true

[dotnet]

project/assembly_name="Twitch"

[filesystem]

import/fbx/enabled=false

[gui]

common/snap_controls_to_pixels=false

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
anti_aliasing/quality/msaa_2d=3
anti_aliasing/quality/msaa_3d=3
viewport/transparent_background=true
anti_aliasing/quality/screen_space_aa=1
anti_aliasing/quality/use_taa=true
